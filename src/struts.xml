		</action>
		<action name="newslistbytag" class="newslist.newslistaction">
			<result name="success">/newscatalogue.jsp</result>
			<result name="fail">/news.jsp</result>
		</action>
		<action name="newslistbydate" class="newslist.newslistaction1">
			<result name="success">/newscatalogue.jsp</result>
			<result name="fail">/news.jsp</result>
		</action>		
		<action name="addnews" class="addnews.addnewsaction">
			<result name="success">/admin_index.jsp</result>
			<!-- <result name="fail">/portfolio.jsp</result> -->
		</action>
		<action name="newsinHKTWMC" class="newslist.newslistaction2">
			<result name="success">/newscatalogue.jsp</result>
			<result name="fail">/hk_mc_tw.jsp</result>
		</action>
		 
		<action name="newsdetails" class="newsdetails.newsdetailsaction">
